<Window x:Class="TestWpf.PresentationModels.ModelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ModelWindow" Height="300" Width="300">
    <ItemsControl ItemsSource="{Binding Fields}">
        <ItemsControl.ItemTemplate>
            <DataTemplate>
                <Label Content="{Binding FieldDefinition.Identifier}" />
            </DataTemplate>
        </ItemsControl.ItemTemplate>
    </ItemsControl>
</Window>
